<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="passgenerator.myapplication">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/MyMaterialTheme">
        <activity
            android:name=".Main"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Gardar_Datos"
            android:screenOrientation="portrait"/>
        <activity android:name=".Listado"
            android:screenOrientation="portrait"/>
        <activity android:name=".MostrarDatos"
            android:screenOrientation="portrait"/>
        <activity android:name=".Fragment" />
        <activity android:name=".PantallaPreferencias" />
        <activity android:name=".ContrasinalFragment" />
        <activity android:name=".FragmentDialogoContrasinal" />
        <activity android:name=".Fragment_Password" />
        <activity android:name=".BorrarFragment" />
        <activity android:name=".Fragment_Enter_Pass" />
        <activity android:name=".Fragment_Change_Data"></activity>
    </application>

</manifest>